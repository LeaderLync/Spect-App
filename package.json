    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.2.3",
    "jquery": "^3.4.0",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "popper.js": "^1.16.0",
